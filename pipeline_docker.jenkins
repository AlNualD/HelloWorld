pipeline {
  agent any
  stages {
    stage ("get code from git") {
    steps {
            // Get some code from a GitHub repository
            git 'https://github.com/AlNualD/HelloWorld.git'
         }
    }
    stage ("docker build") {
      steps {
      echo "++++++++ statr build +++++"
      sh 'docker build . -t hw_in_docker'
      }
    }
    stage ("docker run") {
      steps {
      echo "++++++ docker run +++++++"
      sh 'docker run hw_in_docker'
      }
    }
  }
}  
